package tp2_classe;

public class Cercle {
	
	public Cercle(Point centre,int rayon) {
		private Point centre=new Point(p);
		private int rayon;	
	}
	public String toString() {
		return this.centre.getAbsicce()+","+this.centre.getOrdonne()+"rayon"+this.rayon;
	}
	public void translater(int dx,int dy) {
		this.centre.deplacer(dx, dy);
	}
}
