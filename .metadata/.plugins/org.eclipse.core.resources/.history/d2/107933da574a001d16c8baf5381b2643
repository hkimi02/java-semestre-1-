package tp2_classe;

public class Cercle {
	private Point centre;
	private int rayon;	
	public Cercle(Point p,int rayon) {
		this.centre=new Point(p);
	}
	public String toString() {
		return this.centre.getAbsicce()+","+this.centre.getOrdonne()+"rayon"+this.rayon;
	}
	public void translater(int dx,int dy) {
		this.centre.deplacer(dx, dy);
	}
}
